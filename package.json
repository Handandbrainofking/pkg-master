{
	"name": "pkg-master",
	"version": "1.0.1",
	"description": "pkg-master",
	"keywords": [
		"pkg-master",
		"pkg",
		"master",
		"project",
		"module",
		"package",
		"create",
		"publish"
	],
	"homepage": "https://github.com/JowayYoung",
	"license": "MIT",
	"author": {
		"name": "Joway Young",
		"email": "young.joway@aliyun.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/JowayYoung/pkg-master"
	},
	"bin": {
		"pkg-master": "bin/pkg-master"
	},
	"scripts": {
		"kill": "tskill node",
		"remove": "rimraf node_modules && rimraf package-lock.json && rimraf yarn.lock"
	},
	"dependencies": {
		"chalk": "2.4.1",
		"commander": "2.19.0",
		"execa": "1.0.0",
		"figures": "2.0.0",
		"inquirer": "6.2.0",
		"latest-version": "4.0.0",
		"listr": "0.14.3",
		"ora": "^3.0.0",
		"rimraf": "2.6.2"
	},
	"devDependencies": {
		"eslint": "5.9.0",
		"eslint-config-standard": "12.0.0",
		"eslint-plugin-import": "2.14.0",
		"eslint-plugin-node": "8.0.0",
		"eslint-plugin-promise": "4.0.1",
		"eslint-plugin-standard": "4.0.0"
	},
	"engines": {
		"node": ">= 8.0.0",
		"npm": ">= 5.0.0"
	},
	"eslintConfig": {
		"env": {
			"browser": true,
			"commonjs": true,
			"es6": true,
			"node": true
		},
		"extends": "standard",
		"parserOptions": {
			"ecmaVersion": 9,
			"sourceType": "module"
		},
		"root": true,
		"rules": {
			"eol-last": [
				"error",
				"never"
			],
			"handle-callback-err": [
				"error",
				"error"
			],
			"indent": [
				"error",
				"tab"
			],
			"no-console": [
				"off"
			],
			"no-new": [
				"off"
			],
			"no-tabs": [
				"off"
			],
			"no-unreachable": [
				"off"
			],
			"object-curly-spacing": [
				"error",
				"always"
			],
			"operator-linebreak": [
				"error",
				"before"
			],
			"quotes": [
				"error",
				"double"
			],
			"semi": [
				"error",
				"always"
			],
			"space-before-function-paren": [
				"error",
				"never"
			]
		}
	}
}